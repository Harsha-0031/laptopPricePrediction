{
    "version": 2,
    "builds":[
        {
            "src": "app.py", 
            "use": "@vercel/python",
             "config": {
                "runtime": "python3.10"  // Specify the desired Python version
              }
        }
    ],
    "routes":[
        {"src": "/(.*)", "dest": "app.py"}
    ]
}
